<wx:sub-section>
  {{#> header-skills }}
    {{> header-simple _title="SKILLS" _section="skills" }}
    {{/header-skills}}

    <wx:sub-section>
      <w:tbl>
        <w:tblPr>
          <w:tblW w:w="0" w:type="auto" />
          <w:tblLook w:val="04A0" />
        </w:tblPr>
        <w:tblGrid>
          <w:gridCol w:w="2093" />
          <w:gridCol w:w="7483" />
        </w:tblGrid>
        {{#each r.skills.sets}}
          <w:tr wsp:rsidR="00427ECC" wsp:rsidTr="004A1399">
            <w:tc>
              <w:tcPr>
                <w:tcW w:w="2093" w:type="dxa" />
                <w:shd w:val="clear" w:color="auto" w:fill="auto" />
              </w:tcPr>
              <w:p wsp:rsidR="00427ECC" wsp:rsidRDefault="00FA3357" wsp:rsidP="00427ECC">
                <w:r>
                  <w:t>{{ name }}:</w:t>
                </w:r>
              </w:p>
            </w:tc>
            <w:tc>
              <w:tcPr>
                <w:tcW w:w="7483" w:type="dxa" />
                <w:shd w:val="clear" w:color="auto" w:fill="auto" />
              </w:tcPr>
              <w:p wsp:rsidR="00427ECC" wsp:rsidRDefault="00FA3357" wsp:rsidP="00427ECC">
                <w:pPr>
                  <w:jc w:val="left" />
                </w:pPr>
                <w:r>
                  <w:t>{{#each skills }}{{ this }}{{#unless @last}}, {{/unless}}{{/each}}</w:t>
                </w:r>
              </w:p>
            </w:tc>
          </w:tr>
        {{/each}}
      </w:tbl>
    </wx:sub-section>
</wx:sub-section>